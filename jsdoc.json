{
    "source": {
        "include": ["source"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./out/"
    }
}
